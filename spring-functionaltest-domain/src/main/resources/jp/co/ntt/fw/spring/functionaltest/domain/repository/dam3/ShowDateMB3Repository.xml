<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="jp.co.ntt.fw.spring.functionaltest.domain.repository.dam3.ShowDateMB3Repository">
	
	<resultMap id="dateMap" type="DateMB3" >
	</resultMap>
	
	  <select id="selectDate" parameterType="String"
		resultMap="dateMap">
		<bind name="todoId" value="_parameter" />
		SELECT
		todo_id AS "id"
		,created_at  AS "localDate"
		FROM
		t_todo
		WHERE
		todo_id = #{todoId}
	</select>    
</mapper>
